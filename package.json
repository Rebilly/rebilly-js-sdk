{
  "name": "rebilly-js-sdk",
  "version": "24.1.2",
  "description": "Official Rebilly API JS library for the browser and Node",
  "browser": "./dist/rebilly-js-sdk.js",
  "main": "./dist/rebilly-js-sdk.node.js",
  "types": "./typings/rebilly-js-sdk/index.d.ts",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "build:prod": "npm run clean && webpack --env=prod",
    "build:dev": "npm run clean && webpack --env=dev",
    "prepublish": "npm run build:prod",
    "unit": "mocha --require babel-register --require babel-polyfill --colors ./test/unit/specs/*.spec.js",
    "unit:watch": "npm run unit -- --watch",
    "coverage": "npm run clean && cross-env NODE_ENV=test nyc npm run unit",
    "test": "npm run unit",
    "generate-rebilly-typesOnline": "npx openapi-typescript https://api.redoc.ly/registry/rebilly/combined/combined/bundle/master/openapi.json --output ./typings/rebilly/generated.d.ts",
    "generate-rebilly-types": "npx openapi-typescript ./schema.json --camelCase --output ./typings/rebilly/generated.d.ts",
    "generate-sdk-types": "node ./scripts/generate-sdk-types",
    "tsc:comment-headers": "node ./scripts/comment-headers.js",
    "generate-storefront-types": "npx openapi-typescript https://api.redoc.ly/registry/rebilly/storefront/storefront/bundle/master/openapi.json --output ./typings/rebilly-storefront/generated.d.ts",
    "tsc": "npx tsc -p jsconfig-tsc.json",
    "tsc:watch": "npx tsc -w -p jsconfig-tsc.json",
    "tsc:simple": "npx tsc -p tsc-simple.json --typeRoots typings",
    "tsc:fix-type-name": "node ./scripts/fix-type-name.js",
    "tsc:copy-types": "rm -rf ../frontend-recomm/typings && cp -rf ./typings ../frontend-recomm/typings",
    "tsc:bundle-types": "yarn generate-rebilly-typesOnline && yarn generate-sdk-types && yarn tsc:comment-headers && yarn tsc:simple && yarn tsc:fix-type-name && yarn tsc:copy-types"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rebilly/rebilly-js-sdk.git"
  },
  "keywords": [
    "Rebilly",
    "js",
    "sdk",
    "library",
    "node"
  ],
  "files": [
    "index.js",
    "dist"
  ],
  "author": "Andrei Moldoveanu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rebilly/rebilly-js-sdk/issues"
  },
  "homepage": "https://github.com/Rebilly/rebilly-js-sdk#readme",
  "devDependencies": {
    "axios-mock-adapter": "^1.9.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.1.0",
    "clone-deep": "^1.0.0",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "faker": "^4.1.0",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "openapi-typescript": "2.5.0",
    "rebilly": "file:./typings",
    "rimraf": "^2.6.1",
    "sinon": "^7.2.5",
    "sinon-chai": "^3.3.0",
    "terser-webpack-plugin": "^1.4.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "babel-runtime": "^6.25.0",
    "shortid": "^2.2.14"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "src/resources",
      "test"
    ]
  }
}
