{
  "name": "rebilly-js-sdk",
  "version": "31.1.1",
  "description": "Official Rebilly API JS library for the browser and Node",
  "browser": "./dist/rebilly-js-sdk.js",
  "main": "./dist/rebilly-js-sdk.node.js",
  "types": "./typings/rebilly-js-sdk/index.d.ts",
  "scripts": {
    "clean": "rimraf dist && rimraf coverage",
    "build:prod": "npm run clean && webpack --env=prod",
    "build:dev": "npm run clean && webpack --env=dev",
    "prepublish": "npm run build:prod",
    "test:unit": "jest",
    "test:unit:coverage": "jest --collect-coverage --no-cache",
    "test:unit:debug": "node --inspect-brk jest --no-cache --runInBand",
    "test:unit:watch": "jest --watchAll",
    "test": "npm run test:unit",
    "tsc": "npx tsc -p jsconfig-tsc.json",
    "tsc:watch": "npx tsc -w -p jsconfig-tsc.json",
    "ts:generate-rebilly-types": "npx openapi-typescript https://api.redoc.ly/registry/rebilly/combined/combined/bundle/master/openapi.json --output ./typings/rebilly/generated.d.ts",
    "ts:generate-sdk-types": "node ./scripts/generate-sdk-types",
    "ts:comment-headers": "node ./scripts/comment-headers.js",
    "ts:extract-types": "npx tsc -p tsc-emit.json --typeRoots typings",
    "ts:fix-type-name": "node ./scripts/fix-type-name.js",
    "ts:copy-types": "rm -rf ../frontend-recomm/typings && cp -rf ./typings ../frontend-recomm/typings",
    "ts:bundle-types": "yarn ts:generate-rebilly-types && yarn ts:generate-sdk-types && yarn ts:comment-headers && yarn ts:extract-types && yarn ts:fix-type-name && yarn ts:copy-types",
    "ts:generate-sdk-code": "node ./scripts/sdk-generation/generate-sdk-code"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rebilly/rebilly-js-sdk.git"
  },
  "keywords": [
    "Rebilly",
    "js",
    "sdk",
    "library",
    "node"
  ],
  "files": [
    "index.js",
    "dist"
  ],
  "author": "Andrei Moldoveanu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Rebilly/rebilly-js-sdk/issues"
  },
  "homepage": "https://github.com/Rebilly/rebilly-js-sdk#readme",
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "axios-mock-adapter": "^1.9.0",
    "babel-loader": "^8.2.2",
    "chai": "^4.1.0",
    "clone-deep": "^1.0.0",
    "cross-env": "^5.0.1",
    "dotenv": "^4.0.0",
    "faker": "^4.1.0",
    "jest": "^26.6.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.kebabcase": "^4.1.1",
    "mocha": "^4.0.1",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "openapi-typescript": "2.5.0",
    "prettier": "^2.2.1",
    "rebilly": "file:./typings",
    "rimraf": "^2.6.1",
    "sinon": "^7.2.5",
    "sinon-chai": "^3.3.0",
    "terser-webpack-plugin": "^1.4.1",
    "typescript": "^4.1.3",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-runtime": "^6.25.0",
    "shortid": "^2.2.14"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": false,
    "instrument": false,
    "exclude": [
      "src/resources",
      "test"
    ]
  }
}
