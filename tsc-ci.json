{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es2017",
      "dom"
    ],
    "noEmit": false,
    "allowJs": true,
    "checkJs": true,
    "emitDeclarationOnly": false,
    "declaration": false,
    "outDir": "tmp",
    "removeComments": true,
    "strict": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "types": [
      "./typings/rebilly"
    ],
    "esModuleInterop": true
  },
  "include": [
    "dist/rebilly-js-sdk.d.ts"
  ]
}